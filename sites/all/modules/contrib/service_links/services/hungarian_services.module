function hungarian_services_service_links() {
  $links = array();

  $links['_hu_iwiw'] = array(
    'name' => 'IWIW',
    'description' => t('Add to iWiW.'),
    'link' => 'http://iwiw.hu/pages/share/share.jsp?u=<encoded-url>&t=<encoded-title>',
  );

  return $links;
}
